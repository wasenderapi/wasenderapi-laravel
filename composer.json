{
    "name": "wasenderapi/wasenderapi-laravel",
    "description": "Laravel SDK for WasenderAPI  with event dispatching and webhook route",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "illuminate/support": ">=9.0",
        "illuminate/http": ">=9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "WasenderApi\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WasenderApi\\WasenderApiServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
